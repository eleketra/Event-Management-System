package main;
import java.sql.*;
public class example {

	public static void main(String[] args) {
		try{
			Class.forName("com.mysql.jdbc.Driver");
			Connection con=DriverManager.getConnection("jdbc:mysql://localhost:3306/student","ankita","ankita");
			Statement stmt=con.createStatement();
			ResultSet rst=stmt.executeQuery("select * from student");
			while(rst.next())
			{
				System.out.println(rst.getInt("Student_Id")+" "+rst.getString("fname")+" "+rst.getString("lname"));
			}
			con.close();
		}
		catch(Exception e)
		{
			System.out.println("error "+e);
		}

	}

}
